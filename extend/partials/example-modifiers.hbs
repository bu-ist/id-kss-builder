<div class="id-kss-info-wrapper">
{{#each modifiers}}
	<div class="id-kss-class-name id-kss-modifier-name id-kss-style">
		{{name}}
	</div>
	<p class="id-kss-modifier-description id-kss-style">
		{{{description}}}
	</p>
	<div class="id-kss-modifier-example">
		<div class="id-kss-example-wrapper">
			<iframe class="id-kss-example-iframe" scrolling="no"></iframe>
			<script type="text/template" class="template-iframe">
				{{#>iframetemplate markup=markup classlist=../classes}}
					{{>iframetemplate-base markup=../markup classlist=../classes}}
				{{/iframetemplate}}
			</script>

			<div class="id-kss-example-iframe-handle"></div>
		</div>
		<div class="id-kss-modifier-example-footer"></div>
	</div>
{{/each}}
</div>
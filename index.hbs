<!DOCTYPE html>
<html class="no-js" lang="en">
<head>
	<meta charset="utf-8">
	<title>{{options.title}}</title>

	<meta name="description" content="">
	<meta name="generator" content="id-kss-node">
	<meta name="viewport" content="width=device-width">

	<link rel="stylesheet" href="kss-assets/kss.css">


</head>
<body id="id-kss-node" {{#if template.isItem}}class="id-kss-fullscreen-mode"{{/if}}>
<script type="text/javascript">
	//load the theme stylesheet as a variable to be used
	//by iframe.js to add stylesheets to each iframe example
	var themeStylesheetURL = '{{{options.css}}}';
	var exampleStylesheetURL = '{{{options.exampleStylesheetURL}}}';
	var themeScriptsURL = '{{{options.js}}}';
</script>


<!-- Debug -->
{{>builder-dev-output}}
<!-- /Debug -->


<!-- Header -->
{{>builder-header}}
<!-- /Header -->
<!-- Sidebar -->
{{>builder-sidebar}}
<!-- /Sidebar -->
<main class="id-kss-main" aria-label="Content">
	<article>
	{{#if template.isHomepage}}

		<!-- Homepage Template -->
		{{>builder-homepage}}
		<!-- /homepage -->

	{{else}}


	{{!
		Display each section, in order.

		The "root" element comes first in this loop, and can be detected using
		the "#if @first" block as seen below.
	}}
	{{#each sections}}

		{{!
			Only output this item if it is not set as hidden
			this is useful so we can hide titles for sub categories or things we don't
			want visible yet
		}}
		{{#unless hidden}}
		<{{#if @first}}div{{else}}section{{/if}} id="kssref-{{referenceURI}}" class="id-kss-section id-kss-section--depth-{{depth}} {{#if @root.template.isItem}}is-fullscreen{{/if}}">
			<div class="id-kss-style id-kss-component-content">
				<header class="id-kss-title-block">
					<h{{#ifEquals depth '1' }}1{{else}}2{{/ifEquals}} class="id-kss-title id-kss-title--level-{{depth}} {{#if is-subheader}}id-kss-title-subheader{{/if}}">

						{{#if is-subheader}}
						<span class="id-kss-title-section">{{splitTitle reference 1}}</span>
						{{else}}
						<span class="id-kss-title-section">{{splitTitle reference 2}}</span>
						{{/if}}

						<span class="id-kss-title-name">{{header}}<a class="id-kss-title__permalink" href="#kssref-{{referenceURI}}">Link</a></span>
					</h{{#ifEquals depth '1' }}1{{else}}2{{/ifEquals}}>
					{{#if status}}
						<span class="id-kss-status id-kss-status-{{{slug status}}}">
							{{status}}
						</span>
					{{/if}}

					{{#if is-header}}
						{{! do nothing}}
					{{else}}
						{{#if is-subheader}}
							{{! do nothing}}
						{{else}}
							{{#if @root.options.gitURL}}
							<span class="id-kss-open-github-issue">
								<a href="{{@root.options.gitURL}}/issues/new" class="button button-small" title="Opens a new Issue on Github">New Issue</a>
							</span>
							{{/if}}
						{{/if}}
					{{/if}}

					<!--<span class="id-kss-title__ref">
						{{referenceNumber}}
						<span class="id-kss-title__permalink-hash">
							{{#if @root.hasNumericReferences}}
								#permalink
							{{else}}
								#{{reference}}
							{{/if}}
						</span>
					</span>-->
				</header>
				{{#if description}}
					<div class="id-kss-description">
						{{{description}}}
					</div>
				{{/if}}

				{{#if colors }}
					<ul class="id-kss-colors">
						{{#each colors}}
							<li>
								<figure class="id-kss-color">
									<div class="id-kss-color-figure" style="background-color:{{color}}"></div>
									<figcaption class="id-kss-color-caption">
										{{#if name }}
											<strong class="id-kss-color-name">{{ color.name }}</strong>
										{{/if}}
										<strong class="id-kss-color-name">{{name}}</strong>
										<code class="id-kss-color-code">{{color}}</code>
										{{#if description}}
											<p class="id-kss-color-desc">{{description}}</p>
										{{/if}}
									</figcaption>
								</figure>
							</li>
						{{/each}}
					</ul>
				{{/if}}

				{{#if parameters}}
					<div class="id-kss-parameters__title">Parameters:</div>
					<ul class="id-kss-parameters">
					{{#each parameters}}
						<li class="id-kss-parameters__item">
							<div class="id-kss-parameters__name"><code>{{name}}</code></div>
							<div class="id-kss-parameters__description">
								{{{description}}}
								{{#if defaultValue}}
									<div class="id-kss-parameters__default-value">
										Defaults to: <code>{{defaultValue}}</code>
									</div>
								{{/if}}
							</div>
						</li>
					{{/each}}
					</ul>
				{{/if}}
			</div>

			{{#if example}}
				<div class="id-kss-modifier__wrapper">
					<div class="id-kss-modifier-heading id-kss-docs-heading id-kss-style">
						Example{{#if modifiers}}s{{/if}}

						{{#if source.filename}}
							<span class="id-kss-source id-kss-style">
								Source: <code>{{source.filename}}, line {{source.line}}</code>
								{{#if @root.options.gitURLCSSDEV}}
									<a href="{{@root.options.gitURLCSSDEV}}{{source.filename}}#L{{source.line}}">
										View Source
									</a>
								{{/if}}
							</span>
						{{/if}}
					</div>




						{{#if @root.options.themes}}
							{{>example-tabs-header}}
							{{>example-loop-tabs}}
							</div><!--./id-kss-example-tabs -->
						{{else}}

							{{>example-base modifiers=modifiers example=example}}

							{{>example-modifiers modifiers=modifiers markup=markup}}
						{{/if}}

				</div>

				{{#if markup}}
					<details class="id-kss-markup id-kss-style" open="">
						<summary class="id-kss-docs-heading">
							{{#if markupFile }}
								Markup <code>{{markupFile}}</code>
							{{else}}
								Markup
							{{/if}}
						</summary>
						<pre class="hljs"><code class="language-handlebars">{{{hljs markup 'handlebars'}}}</code></pre>
					</details>
				{{/if}}
			{{/if}}

		</{{#if @first}}div{{else}}section{{/if}}>
		{{/unless}}
	{{/each}}
{{/if}}
</article>
</main>
<!-- SCRIPTS -->
<script src="kss-assets/kss.js"></script>
<script src="kss-assets/scrollspy.js"></script>
<script src="kss-assets/kss-fullscreen.js"></script>
<script src="kss-assets/kss-guides.js"></script>
<script src="kss-assets/kss-markup.js"></script>
<script src="kss-assets/kss-breakpoints.js"></script>
<script src="kss-assets/kss-sidebar.js"></script>
<script src="kss-assets/jquery.js"></script>
<script src="kss-assets/jquery-resizable.js"></script>
<script src="kss-assets/iframe.js"></script>
<script src="kss-assets/mark.min.js"></script>
<script src="kss-assets/kss-search.js"></script>

<script type="text/javascript">
	var spy = new ScrollSpy('#id-kss-node', {
		nav: '.id-kss-nav-menu-child > li > a',
		className: 'is-in-viewport'
	});
	var kssFullScreen = new KssFullScreen({
		idPrefix: 'id-kss-fullscreen-',
		bodyClass: 'id-kss-fullscreen-mode',
		elementClass: 'is-fullscreen'
	});
	var kssSidebar = new KssSidebar({
		bodyClass: 'id-kss-sidebar-open',
	});
	var kssGuides = new KssGuides({
		bodyClass: 'id-kss-guides-mode'
	});
	var kssBreakpoints = new KssBreakpoints({
		bodyClass: 'id-kss-breakpoint-xs'
	});
	var kssMarkup = new KssMarkup({
		bodyClass: 'id-kss-markup-mode',
		detailsClass: 'id-kss-markup'
	});
	var kssSearch = new KssSearch({
		inputID: 'kss-search-field',
		searchButtonID: 'kss-search-button',
    	searchAreaClass: '.id-homepage-nav'
	});

</script>

{{! This will only be included in the demo page. }}
{{#if options.demo}}
	<footer class="id-kss-github">
		<!-- https://github.com/blog/273-github-ribbons -->
		<a href="https://github.com/id-kss-node/id-kss-node"><img src="kss-assets/github-fork--black.png" alt="Fork me on GitHub"></a>
	</footer>
{{/if}}

<!-- Automatically built using <a href="https://github.com/kss-node/kss-node">kss-node</a>. -->
</body>
</html>
